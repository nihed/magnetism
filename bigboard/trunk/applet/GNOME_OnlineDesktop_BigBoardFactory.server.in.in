<oaf_info>

<oaf_server iid="OAFIID:GNOME_OnlineDesktop_BigBoardFactory"
	    type="@TYPE@"
	    location="@LOCATION@">

	<oaf_attribute name="repo_ids" type="stringv">
		<item value="IDL:Bonobo/GenericFactory:1.0"/>
		<item value="IDL:Bonobo/Unknown:1.0"/>
	</oaf_attribute>
	<oaf_attribute name="name" type="string" _value="BigBoard Applet Factory"/>
	<oaf_attribute name="description" type="string" _value="Factory for the BigBoard-related applets"/>

</oaf_server>

<oaf_server iid="OAFIID:GNOME_OnlineDesktop_BigBoardButtonApplet"
	    type="factory" 
	    location="OAFIID:GNOME_OnlineDesktop_BigBoardFactory">

	<oaf_attribute name="repo_ids" type="stringv">
		<item value="IDL:GNOME/Vertigo/PanelAppletShell:1.0"/>
		<item value="IDL:Bonobo/Control:1.0"/>
		<item value="IDL:Bonobo/Unknown:1.0"/>
	</oaf_attribute>
	<oaf_attribute name="name" type="string" _value="Show Sidebar Button"/>
	<oaf_attribute name="description" type="string" _value="Show and hide the desktop sidebar"/>
	<oaf_attribute name="panel:icon" type="string" value="gnome-fs-desktop.png"/>
        <!-- bugzilla properties can go here once we have a bugzilla... -->
</oaf_server>

</oaf_info>
