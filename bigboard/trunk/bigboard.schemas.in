<!-- -*- mode: xml -*- -->
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/bigboard/focus_key</key>
      <applyto>/apps/bigboard/focus_key</applyto>
      <owner>bigboard</owner>
      <type>string</type>
      <default>Super_L</default>
      <locale name="C">
        <short>Key name for focusing (and toggling popout state)</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/bigboard/expand</key>
      <applyto>/apps/bigboard/expand</applyto>
      <owner>bigboard</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
		<short>Whether to use expanded size</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/bigboard/visible</key>
      <applyto>/apps/bigboard/visible</applyto>
      <owner>bigboard</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
		<short>Whether to minimize to the panel</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/bigboard/url_listings</key>
      <applyto>/apps/bigboard/url_listings</applyto>
      <owner>bigboard</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[builtin://files.xml,builtin://apps.xml,builtin://people.xml]</default>
      <locale name="C">
		<short>The list of visible stocks; supersedes old listings key.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/bigboard/first_time_minimize_seen</key>
      <applyto>/apps/bigboard/first_time_minimize_seen</applyto>
      <owner>bigboard</owner>
      <type>bool</type>
      <default>FALSE</default>
      <locale name="C">
		<short>Whether to show dialog on first minimization</short>
      </locale>
    </schema>    
    <schema>
      <key>/schemas/apps/bigboard/application_list_size</key>
      <applyto>/apps/bigboard/application_list_size</applyto>
      <owner>bigboard</owner>
      <type>int</type>
      <default>7</default>
      <locale name="C">
		<short>Number of applications to show in sidebar</short>
      </locale>
    </schema>        
  </schemalist>
</gconfschemafile>
