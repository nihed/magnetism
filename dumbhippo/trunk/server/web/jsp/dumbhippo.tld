<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" 
"http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
	<tlibversion>0.1</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>dh</shortname>
	<info>DumbHippo Tag library</info>
    <tag>
        <name>bean</name>
        <tagclass>com.dumbhippo.web.tags.BeanTag</tagclass>
        <bodycontent>tagdependent</bodycontent>
        <info>A bean to instantiate with DumbHippo specific injection</info>
	    <attribute>
           <name>id</name>
           <required>true</required>
           <rtexprvalue>false</rtexprvalue>
        </attribute>
	    <attribute>
           <name>class</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
	    <attribute>
           <name>scope</name>
           <required>true</required>
           <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>default</name>
        <tagclass>com.dumbhippo.web.tags.DefaultTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>Provide a default value for a variable if unset</info>
	    <attribute>
           <name>var</name>
           <required>true</required>
           <rtexprvalue>false</rtexprvalue>
        </attribute>
	    <attribute>
           <name>value</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
	    <attribute>
           <name>scope</name>
           <required>false</required>
           <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>entity</name>
        <tagclass>com.dumbhippo.web.tags.EntityTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>A taggified list person or group</info>
	    <attribute>
           <name>value</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>showInviteLinks</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>photo</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>music</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>cssClass</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>bodyLengthLimit</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>     
        <attribute>
           <name>longBodyLengthLimit</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>   
        <attribute>
           <name>twoLineBody</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>      
    </tag>
    <tag>
        <name>entityList</name>
        <tagclass>com.dumbhippo.web.tags.EntityListTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>A taggified list of people and groups</info>
	    <attribute>
           <name>prefixValue</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>        
	    <attribute>
           <name>value</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>skipRecipientId</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>showInviteLinks</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>photos</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>music</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>cssClass</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>bodyLengthLimit</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute> 
        <attribute>
           <name>longBodyLengthLimit</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>separator</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>          
        <attribute>
           <name>twoLineBody</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>                         
    </tag>
    <tag>
    	<name>iterateStack</name>
    	<tagclass>com.dumbhippo.web.tags.IterateStack</tagclass>
    	<bodycontent>scriptless</bodycontent>
    	<info>Iterate over a collection of blocks forming a stack</info>
    	<attribute>
    		<name>items</name>
    		<required>true</required>
    		<rtexprvalue>true</rtexprvalue>
   		</attribute>
    	<attribute>
    		<name>var</name>
    		<required>true</required>
    		<rtexprvalue>false</rtexprvalue>
   		</attribute>
    	<attribute>
    		<name>varStatus</name>
    		<required>true</required>
    		<rtexprvalue>false</rtexprvalue>
   		</attribute>
   	</tag>
    <tag>
    	<name>forEachDownload</name>
    	<tagclass>com.dumbhippo.web.tags.ForEachDownload</tagclass>
    	<bodycontent>scriptless</bodycontent>
    	<info>Iterate over downloads for a TrackView</info>
    	<attribute>
    		<name>track</name>
    		<required>true</required>
    		<rtexprvalue>true</rtexprvalue>
   		</attribute>
    	<attribute>
    		<name>var</name>
    		<required>true</required>
    		<rtexprvalue>false</rtexprvalue>
   		</attribute>
    	<attribute>
    		<name>varStatus</name>
    		<required>true</required>
    		<rtexprvalue>false</rtexprvalue>
   		</attribute>
   	</tag>
    <tag>
        <name>png</name>
        <tagclass>com.dumbhippo.web.tags.PngTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>A png that works in both firefox and IE</info>
	    <attribute>
           <name>src</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>style</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>klass</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>        
        <dynamic-attributes>true</dynamic-attributes>
    </tag>
        <tag>
        <name>jsString</name>
        <tagclass>com.dumbhippo.web.tags.JavascriptStringTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>Output a JavaScript string literal</info>
	    <attribute>
           <name>value</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
    	<name>login</name>
        <tagclass>com.dumbhippo.web.tags.LoginTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>Redirect to /who-are-you</info>
	    <attribute>
           <name>next</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>nowPlaying</name>
        <tagclass>com.dumbhippo.web.tags.NowPlayingTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>Outputs the "now playing" flash badge object</info>
	    <attribute>
           <name>userId</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>themeId</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>escapeXml</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>forceMode</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>hasLabel</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>embedOnly</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        
    </tag>
    <tag>
        <name>flashBadge</name>
        <tagclass>com.dumbhippo.web.tags.FlashBadgeTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>Outputs a flash badge object</info>
        <attribute>
           <name>badge</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>        
	    <attribute>
           <name>userId</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>escapeXml</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>hasLabel</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>embedOnly</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>   
    </tag>
    <tag>
        <name>aRedHatSomething</name>
        <tagclass>com.dumbhippo.web.tags.RedHatSomethingTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>Outputs "A Red Hat Something"</info>        
    </tag>    
    <tag>
        <name>pagerLinkList</name>
        <tagclass>com.dumbhippo.web.tags.PagerLinkListTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>Display list of pager links</info>        
	    <attribute>
           <name>pageable</name>
           <required>true</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
           <name>anchor</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>script</name>
        <tagclass>com.dumbhippo.web.tags.ScriptTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>Script modules to include (automatically pulling in their dependencies)</info>
	    <attribute>
           <name>module</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>
	    <attribute>
           <name>modules</name>
           <required>false</required>
           <rtexprvalue>true</rtexprvalue>
        </attribute>                        
    </tag>    
    <function>
    	<name>enumIs</name>
    	<function-class>com.dumbhippo.web.tags.FuncUtils</function-class>
    	<function-signature>boolean enumIs( java.lang.Enum, java.lang.String )</function-signature>
    </function>
    <function>
    	<name>format1</name>
    	<function-class>com.dumbhippo.web.tags.FuncUtils</function-class>
    	<function-signature>java.lang.String format1(java.lang.String, java.lang.Object )</function-signature>
    </function>
    <function>
    	<name>myInstanceOf</name>
    	<function-class>com.dumbhippo.web.tags.FuncUtils</function-class>
    	<function-signature>boolean myInstanceOf( java.lang.Object, java.lang.String )</function-signature>
    </function>
    <function>
    	<name>randomInt</name>
    	<function-class>com.dumbhippo.web.tags.FuncUtils</function-class>
    	<function-signature>int randomInt( int )</function-signature>
    </function>    
    <function>
    	<name>urlEncode</name>
    	<function-class>com.dumbhippo.web.tags.FuncUtils</function-class>
    	<function-signature>java.lang.String urlEncode(java.lang.String)</function-signature>
    </function>          
    <function>
    	<name>xmlEscape</name>
    	<function-class>com.dumbhippo.web.tags.FuncUtils</function-class>
    	<function-signature>java.lang.String xmlEscape(java.lang.String)</function-signature>
    </function>
    <function>
    	<name>size</name>
    	<function-class>com.dumbhippo.web.tags.FuncUtils</function-class>
    	<function-signature>int size(java.util.Collection)</function-signature>
    </function>
</taglib>
