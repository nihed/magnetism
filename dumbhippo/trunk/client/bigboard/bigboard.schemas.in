<!-- -*- mode: xml -*- -->
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/bigboard/expand</key>
      <applyto>/apps/bigboard/expand</applyto>
      <owner>bigboard</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
	<short>Whether to use expanded size</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/bigboard/visible</key>
      <applyto>/apps/bigboard/visible</applyto>
      <owner>bigboard</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
	<short>Whether to minimize to the panel</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/bigboard/listings</key>
      <applyto>/apps/bigboard/listings</applyto>
      <owner>bigboard</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[org.mugshot.bigboard.SelfStock,org.mugshot.bigboard.SearchStock,org.mugshot.bigboard.AppsStock,org.mugshot.bigboard.PeopleStock,org.mugshot.bigboard.PhotosStock]</default>
      <locale name="C">
	<short>The list of visible stocks</short>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
